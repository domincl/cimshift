{
  "manifest_version": 3,
  "name": "CimShift",
  "version": "1.0",
  "description": "UI tweaks in cim",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "predefined_searches.json"
      ],
      "matches": [
        "https://nextbulletin.temple.edu/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "js": [
        "content_programadmin.js"
      ],
      "matches": [
        "https://nextbulletin.temple.edu/programadmin/*"
      ]
    },
    {
      "js": [
        "content_programadmin_frames.js"
      ],
      "css": [
        "cimshift.css"
      ],
      "all_frames": true,
      "matches": [
        "https://nextbulletin.temple.edu/courseleaf/*"
      ]
    }
  ]
}