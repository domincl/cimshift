{
  "manifest_version": 3,
  "name": "MoreInfo",
  "version": "1.0",
  "description": "UI tweaks in cim",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "predefined_searches.json"
      ],
      "matches": [
        "https://nextbulletin.temple.edu/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "js": [
        "content_programadmin.js"
      ],
      "css": [
        "resize_frames.css"
      ],
      "matches": [
        "https://nextbulletin.temple.edu/programadmin/*"
      ]
    }
  ]
}