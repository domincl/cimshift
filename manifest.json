{
  "manifest_version": 3,
  "name": "CimShift",
  "version": "1.0",
  "description": "UI tweaks in cim",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "cimshift.css"
      ],
      "all_frames": true,
      "matches": [
        "https://nextbulletin.temple.edu/programadmin/*",
        "https://nextbulletin.temple.edu/courseleaf/*"
      ]
    }
  ]
}