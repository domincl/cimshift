{
  "manifest_version": 3,
  "name": " nextbulletin rework UI",
  "version": "1.0",
  "description": "Add predifined searches and simplify web page for Temple University Program Admin web page.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "summary_help.js",
        "add_predefined_searches.js"
      ],
      "css": [
        "resize_frames.css"
      ],
      "matches": [
        "https://nextbulletin.temple.edu/programadmin/*"
      ]
    }
  ]
}